local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Cenozoic Survival", "Serpent")
local Welcome = Window:NewTab("Welcome!")
local Welcome1 = Welcome:NewSection("Welcome to Wowzer's gui, ".. game.Players.LocalPlayer.Name .."/".. game.Players.LocalPlayer.DisplayName .."!")
Welcome1:NewKeybind("UI Toggle", "Toggles the ui on and off.", Enum.KeyCode.Backquote, function()
	Library:ToggleUI()
end)
Welcome1:NewLabel("Credits to Cypher for help with scripting.")
local myTable = {"fr (for real)", "howTOcodeTUTORIAL", "lazy hub real", "hkjgbghouiyoiuiopu9utg76rf76rballshiwstddadgoneforeverformilkuntiliwinalottery", "send nudes!1!", "free bobux no skem", "if syn then haxor alert end"}
Welcome1:NewLabel( myTable[ math.random( #myTable ) ] )
local Client = Window:NewTab("Client")
local Client1 = Client:NewSection("Other")
Client1:NewButton("Chat Spy", "Allows the client to read private messages.", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/9JyKaJEr",true))()
end)
Client1:NewButton("Infinite Yield", "Turns Infinite Yield on.", function()
   loadstring(game:HttpGet(("https://raw.githubusercontent.com/Infinite-Store/Infinite-Store/main/main.lua"), true))()
end)
Client1:NewLabel("Esps currently broken(?) as of 25/03/22")
Client1:NewButton("Unnamed ESP", "Enables Unnamed ESP.", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/LOOF-sys/Roblox-Shit/main/Unnamed%20Esp.lua",true))()
end)
local Client2 = Client:NewSection("Movement")
Client2:NewSlider("Walk speed", "Changes the client's walk speed.", 1000, 0, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Client2:NewSlider("Jump Power", "Changes the client's jump power.", 1000, 0, function(s)
    game.Players.LocalPlayer.Character.Humanoid.Jumppower = s
end)
Client2:NewSlider("Gravity", "Changes the client's gravity.", 500, 0, function(s)
    workspace.Gravity = s
end)
local Client3 = Client:NewSection("Health")
Client3:NewButton("Kill Client", "Kills the client.", function()
        local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Humanoid,
        [2] = math.huge,
        [3] = -0
    }
    game:GetService("ReplicatedStorage").Events.DamageEvent:FireServer(unpack(args))
end)
Client3:NewButton("God Client", "Gives the client god mode.", function()
    local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Humanoid,
        [2] = -math.huge,
        [3] = math.huge
    }
    game:GetService("ReplicatedStorage").Events.DamageEvent:FireServer(unpack(args))
end)
Client3:NewLabel("Put a '-' before the number to heal, not putting a number in will deal damage.")
Client3:NewTextBox("Heal Client", "Heals the client by the inputted value.", function(h)
        local args = {
        [1] = game:GetService("Players").LocalPlayer.Character.Humanoid,
        [2] = h,
        [3] = -0
    }
    game:GetService("ReplicatedStorage").Events.DamageEvent:FireServer(unpack(args))
end)
Client3:NewLabel("The below only works on the testing branch.")
Client3:NewDropdown("Infinite Stats", "Removes your ability to use up the selected.", {"Stamina", "Thirst", "Hunger", "Oxygen"}, function (v)
    local s = game:GetService("Players").LocalPlayer.Name
    game.Workspace[s].Controls.v.Value = math.huge
    game.Players.LocalPlayer.PlayerGui.Stats.Frame.Back[v]:Destroy()
end)
